VAR_TYPE:
{{ template "skip_irrelevant" }}
if i.head >= len(i.str) {
	i.errc = ErrUnexpEOF
	goto ERROR
} else if i.str[i.head] == '#' {
	goto COMMENT
} else if i.str[i.head] == '[' {
	i.tail = -1
	i.token = TokenVarTypeArr
	{{- template "callback" . -}}
	i.head++
	typeArrLvl++
	goto VAR_TYPE
}
i.expect = ExpectVarType
{{ template "name" set . "aftername" "vartype" }}
